                string salesOrderNumber = sqlReader.GetString(3);

                if (orderDate.Month >= 1 && orderDate.Month <= 3) 
                {
                    Quarter1Buffer.AddRow();
                    Quarter1Buffer.OrderDate = orderDate;
                    Quarter1Buffer.SalesOrderID = salesOrderID;
