        String selectStatement = "select OrderDate, SalesOrderID, TotalDue, SalesOrderNumber from Sales.SalesOrderHeader";
        SqlCommand cmd = new SqlCommand(selectStatement, sqlConn);
        sqlReader = cmd.ExecuteReader();
