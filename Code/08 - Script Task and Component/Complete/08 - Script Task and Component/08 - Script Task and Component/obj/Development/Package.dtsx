    class Sale 
    {
        public DateTime OrderDate { get; set; }
        public int SalesOrderID { get; set; }
        public Decimal TotalDue { get; set; }
        public String SalesOrderNumber { get; set; }
    }

    string fileConn;
    private StreamWriter textWriter;
    private List<Sale> sales = new List<Sale>();

    public override void AcquireConnections(object Transaction)
    {
        base.AcquireConnections(Transaction);
        IDTSConnectionManager100 connMgr = this.Connections.FileConnection;
        fileConn = (string)connMgr.AcquireConnection(Transaction);
    }

    public override void PreExecute()
    {
        base.PreExecute();
        textWriter = new StreamWriter(fileConn, false);
    }

     public override void PostExecute()
    {
        base.PostExecute();
        var serializer = new JavaScriptSerializer();
        var serializedResult = serializer.Serialize(sales);
        textWriter.Write(serializedResult);
        textWriter.Close();
    }

   public override void QuarterSales_ProcessInputRow(QuarterSalesBuffer Row)
    {
        sales.Add(new Sale() { 
            OrderDate = Row.OrderDate, 
            SalesOrderID = Row.SalesOrderID, 
            TotalDue = Row.TotalDue, 
            SalesOrderNumber = Row.SalesOrderNumber 
        });
    }

}
